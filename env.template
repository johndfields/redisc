# Environment Configuration Template
# Copy this file to .env.local, .env.development, .env.staging, or .env.production
# and fill in your actual values

# Enable SSH tunnel (true/false)
USE_SSH=true

# SSH Configuration
SSH_HOST=your.ssh.host.com
SSH_PORT=22
SSH_USERNAME=your_username
SSH_PRIVATE_KEY_PATH=/path/to/your/private/key
SSH_PASSPHRASE=your_passphrase_if_any

# Redis Configuration (the forwarded destination)
REDIS_HOST=your.redis.host
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_if_any
REDIS_DB=0